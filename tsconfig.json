{
  "include": [
    "./**/*",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "public"
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "lib": [
      "dom",
      "esnext"
    ],
    "baseUrl": ".",
    "paths": {
      "pages/*": [
        "src/pages/*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "utils/*": [
        "src/utils/*"
      ],
      "constants/*": [
        "src/constants/*"
      ]
    },
    "sourceMap": true,
    "strict": true,
    "allowJs": false,
    "noEmit": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "preserve",
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "outDir": "dist",
    "strictPropertyInitialization": false,
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  }
}
